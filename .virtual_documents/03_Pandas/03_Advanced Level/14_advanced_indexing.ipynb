    ['2023', '2024'],
    ['Q1', 'Q2', 'Q3', 'Q4']
], names=['Year', 'Quarter'])

np.random.seed(42)
revenue = pd.Series(np.random.randint(100, 200, 8), index=index, name='Revenue')
print(revenue)
print()


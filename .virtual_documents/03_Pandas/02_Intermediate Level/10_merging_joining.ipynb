spending_summary = customer_spending.groupby(['customer_id', 'name', 'city']).agg(
    total_spent=('amount', 'sum'),
    num_orders=('order_id', 'count'),
    avg_order=('amount', 'mean')
).round(2).sort_values('total_spent', ascending=False)

